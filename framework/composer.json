{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "barryvdh/laravel-dompdf": "^2.0.1",
        "doctrine/dbal": "^3.6.4",
        "edujugon/push-notification": "^5.3.0",
        "google/apiclient": "^2.15",
        "guzzlehttp/guzzle": "^7.7.0",
        "kodeine/laravel-meta": "^2.2.0",
        "kreait/laravel-firebase": "^5.2.0",
        "laravel-notification-channels/webpush": "^7.1.0",
        "laravel/framework": "^10.0",
        "laravel/helpers": "^1.6.0",
        "laravel/legacy-factories": "^1.3.2",
        "laravel/passport": "^11.8.7",
        "laravel/slack-notification-channel": "^2.5.0",
        "laravel/tinker": "^2.8.1",
        "laravel/ui": "^4.2.2",
        "laravelcollective/html": "6.4.1",
        "maatwebsite/excel": "^3.1.48",
        "minishlink/web-push": "^8.0.0",
        "nunomaduro/collision": "^7.7.0",
        "phpunit/phpunit": "^10.2.3",
        "pusher/pusher-php-server": "^7.2.3",
        "razorpay/razorpay": "2.8.*",
        "sly/notification-pusher": "*",
        "spatie/laravel-backup": "^8.1.11",
        "spatie/laravel-ignition": "^2.2.0",
        "spatie/laravel-permission": "^5.10.2",
        "stripe/stripe-php": "^10.16.0",
        "yajra/laravel-datatables-oracle": "^10.6.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
            
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
